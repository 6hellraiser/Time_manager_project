<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.vsu.cs.timemanagement"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_SETTINGS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ru.vsu.cs.timemanagement.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ru.vsu.cs.timemanagement.List_Activity"
            android:label="@string/title_activity_list_" >
        </activity>
        <activity
            android:name="ru.vsu.cs.timemanagement.Edit_Activity"
            android:label="@string/title_activity_edit_" >
        </activity>
        <activity
            android:name="ru.vsu.cs.timemanagement.Map_activity"
            android:label="@string/title_activity_map_activity" >
        </activity>
    </application>

</manifest>
